<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.halfsummer.management.user.mapper.SysUserMapper">

 	<!-- 根据主键获取单条记录 -->
    <select id="getById" resultType="com.halfsummer.management.user.entity.SysUser">
		SELECT
		*
		FROM `sys_user`
		WHERE name = #{name}
	</select>



</mapper>